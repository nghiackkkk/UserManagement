                                                <div class="invalid-feedback">
                                                    Field required.
                                                </div>
                                            </div>
                                        </div>
                                        <div class="mb-3 row">
                                            <label class="form-label" for="inputAddress">Regular Address</label>
                                            <div class="col-md-2">
                                                <label class="form-label" for="inputAddress">City</label>
                                                <input asp-for="RegularAddressCity" type="text" class="form-control" id="raCity" placeholder="Ha Noi">
                                                <div class="invalid-feedback">
                                                    Field required.
                                                </div>
                                            </div>
                                            <div class="col-md-2">
