    Layout = null;
}
@model UserManagement.Models.Viewsmodel.UserViewModel
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Responsive Admin &amp; Dashboard Template based on Bootstrap 5">
    <meta name="author" content="AdminKit">
    <meta name="keywords" content="adminkit, bootstrap, bootstrap 5, admin, dashboard, template, responsive, css, sass, html, theme, front-end, ui kit, web">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="shortcut icon" href="/template/img/icons/icon-48x48.png" />

    <link rel="canonical" href="https://demo-basic.adminkit.io/pages-blank.html" />


    <title>Add User</title>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
    <link href="/template/css/app.css" rel="stylesheet">
    <link href="/template/css/custom_ShowUser.css" rel="stylesheet">
    <link href="/template/css/light.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;600&display=swap" rel="stylesheet">
</head>

<body>
    <div class="wrapper">
        <partial name="~/Views/Partials/Side_bar.cshtml"></partial>

        <div class="main">
            <partial name="~/Views/Partials/Top_navbar.cshtml"></partial>

            <main class="content">
                <div class="container-fluid p-0">

                    <h1 class="h3 mb-3">Edit User ID  @ViewBag.Id</h1>

                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-body">
                                    <form asp-controller="Admin" asp-action="UpdateUserDo" method="POST" enctype="multipart/form-data">
                                        <div class="row" id="">
                                            <div class="mb-3 col-md-6">
                                                <label asp-for="CoverImage" class="form-label" for="inputEmail4">Cover Image</label>
                                                <input id="imgInp" onchange="previewImg()" asp-for="CoverImage" class="form-control" type="file" value="@ViewBag.CoverImage">

                                                <div class="invalid-feedback">
                                                    Field required.
                                                </div>
                                            </div>
                                            <div id="add-photo" class="mb-3 col-md-6 d-flex flex-row justify-content-center align-items-center">
                                                <img id="img-show" class="previewImage object-fit-cover" src="/uploads/@ViewBag.CoverImage" />
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="mb-3 col-md-6">
                                                <label class="form-label" for="username">Username</label>
                                                <input onblur="validateUsernameU()" asp-for="Username" type="text" class="form-control"
                                                       id="username" placeholder="Username">
                                                <div class="invalid-feedback">
                                                    Username are already used.
                                                </div>
                                            </div>
                                            <div class="mb-3 col-md-6">
                                                <label class="form-label" for="inputPassword">Password</label>
                                                <div class="row">
                                                    <input asp-for="Password" type="password"
                                                           class="form-control col-md-10 w-75"
                                                           id="inputPassword"
                                                           placeholder="Password"
                                                           onkeyup="validatePassword()"
                                                           value="@Model.Password">

                                                    <span onclick="revealPassword()"
                                                          class="btn btn-primary col-md-2"
                                                          style="margin-left: 10px !important">Show/Hide</span>
                                                </div>

                                                <div class="invalid-feedback">
                                                    Password must include uppercase, lowercase, number, 3-10 letter and special symbol.
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="mb-3 col-md-6">
                                                <label class="form-label" for="fullname">Full Name</label>
                                                <input asp-for="FullName"
                                                       type="text"
                                                       class="form-control "
                                                       id="fullname"
                                                       placeholder="e.g. Croward Admin"
                                                       value="@Model.FullName">
                                                <div class="invalid-feedback">
                                                    Name seem unreal.
                                                </div>
                                            </div>
                                            <div class="mb-3 col-md-3">
                                                <label class="form-label" for="gender">Gender</label>
                                                <select asp-for="Gender" class="form-select flex-grow-1">
                                                    <option>Male</option>
                                                    <option>Female</option>
                                                </select>
                                            </div>
                                            <div class="mb-3 col-md-3">
                                                <label class="form-label" for="dateOfBirth">Date of birth</label>
                                                <input id="dob" asp-for="DateOfBirth" type="date" class="form-control ">
                                                <div class="invalid-feedback">
                                                    Field required.
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="mb-3 col-md-4">
